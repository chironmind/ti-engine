{
  "name": "ti-engine",
  "version": "1.1.2",
  "description": "WASM bindings for RustTI technical indicators for Node and browser",
  "author": "ChironMind",
  "license": "MIT",
  "type": "module",
  "homepage": "https://chironmind.github.io/ti-engine/",
  "private": false,
  "files": [
    "dist/",
    "index.js",
    "index.web.js",
    "index.node.js",
    "index.d.ts",
    "README.md"
  ],
  "scripts": {
    "build:web": "wasm-pack build --release --target web --out-dir dist/web && node -e \"try{require('fs').rmSync('dist/web/.gitignore')}catch(e){}\"",
    "build:node": "wasm-pack build --release --target nodejs --out-dir dist/node && node -e \"try{require('fs').rmSync('dist/node/.gitignore')}catch(e){}\"",
    "build:bundler": "wasm-pack build --release --target bundler --out-dir dist/bundler && node -e \"try{require('fs').rmSync('dist/bundler/.gitignore')}catch(e){}\"",
    "build": "npm run build:web && npm run build:node && npm run build:bundler",
    "prepublishOnly": "npm run build",
    "test": "npm run build && node --test"
  },
  "keywords": [
    "rust",
    "wasm",
    "technical-indicators",
    "trading",
    "quant"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ChironMind/ti-engine"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "browser": "./index.web.js",
      "require": "./index.node.js",
      "import": "./index.js"
    }
  },
  "types": "./index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  }
}
